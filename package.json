{
  "name": "etimo-kubernetes",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:Etimo/etimo-kubernetes.git",
  "author": "Daniel Winther <daniel.winther@etimo.se>",
  "license": "MIT",
  "scripts": {
    "all": "yarn all:pre-terraform && yarn all:post-terraform",
    "render:projects:terraform": "ts-node ./scripts/bin/render-projects-terraform.ts",
    "render:projects:kubernetes": "ts-node ./scripts/bin/render-projects-kubernetes.ts",
    "render:owners": "ts-node ./scripts/bin/render-owners.ts",
    "render:csr": "ts-node ./scripts/bin/render-csr.ts",
    "sync": "yarn sync:projects && yarn sync:users && yarn sync:migrations",
    "sync:users": "ts-node ./scripts/bin/sync-users.ts",
    "sync:projects": "ts-node ./scripts/bin/sync-projects.ts",
    "sync:cluster-info": "ts-node ./scripts/bin/sync-cluster-info.ts",
    "sync:migrations": "ts-node ./scripts/bin/sync-migrations.ts",
    "clean": "./scripts/bin/clean.sh",
    "download-user-list": "./scripts/bin/download-user-list.sh",
    "send:kubeconfigs": "ts-node ./scripts/bin/send-kubeconfigs.ts",
    "all:pre-terraform": "yarn clean && yarn download-user-list && yarn render:owners && yarn render:projects:terraform",
    "all:post-terraform": "yarn sync:cluster-info && yarn render:projects:kubernetes && yarn sync",
    "validate": "yarn clean && yarn all:pre-terraform"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@types/glob": "^7.2.0",
    "@types/node": "^17.0.22",
    "@types/nodemailer": "^6.4.4",
    "@types/shelljs": "^0.8.11",
    "chalk": "^4.1.2",
    "commander": "^9.0.0",
    "glob": "^7.2.0",
    "handlebars": "^4.7.7",
    "hbs-cli": "^1.4.1",
    "husky": "^7.0.4",
    "joi": "^17.6.0",
    "js-base64": "^3.7.2",
    "nodemailer": "^6.7.2",
    "prettier": "^2.6.0",
    "pretty-quick": "^3.1.3",
    "shelljs": "^0.8.5",
    "terraform-generator": "^5.1.1",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.2",
    "yaml": "^1.10.2"
  }
}
